<!DOCTYPE html>
<html>
<head>
	<title>ITMAGICAL - Easy video conversation</title>
    <link rel="icon" type="image/png" href="/images/favicon-icon.png">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="/css/style.css">
</head>
<body id="background_body">
    <div class="col-md-12">
        <div class="col-md-9">
            <div id="content">
                
            </div>
        </div>
            <div class="col-md-3">

                <div id="live-chat">
                
                <header class="clearfix">
                    
                    <span class="chat-close">x</span>

                    <h4 class="room-name"></h4>

                </header>

                <div class="chat">
                    
                    <div class="chat-history">

                    </div> <!-- end chat-history -->

                    <!-- <p class="chat-feedback">Your partner is typing…</p> -->

                    <div class="form-type-message">

                        <fieldset>
                            
                            <input type="text" class="type-message" placeholder="Type your message…" autofocus>
                            <canvas id="canvas" hidden></canvas>

                        </fieldset>

                    </div>

                </div> <!-- end chat -->

            </div> <!-- end live-chat -->
        </div>
    </div>
    <div class="menu-control">
        <span class="stickers">
            <img class="img-responsive" src="/images/stickers.png" alt="Stickers">
            Stickers
        </span>
        <span class="view-all"><img class="img-responsive" src="/images/screen.png" alt="View All">
            View All
        </span>
        <span class="mic"><img class="img-responsive" src="/images/mic.png" alt="Microphone">
            Mic off
        </span>
        <span class="cam"><img class="img-responsive" src="/images/camera.png" alt="Camera">
            Cam off
        </span>
        <span class="leave"><img class="img-responsive" src="/images/leave.png" alt="Leave">
            Leave
        </span>
    </div>
</body>
<script src="/socket.io/socket.io.js"></script>
<script src="/bundle.js"></script>
</html>